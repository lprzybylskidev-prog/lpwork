{
    "name": "lpwork/framework",
    "description": "LPwork PHP Framework",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "≈Åukasz Przybylski",
            "email": "lprzybylskidev@gmail.com",
            "role": "Creator"
        }
    ],
    "require": {
        "php": "^8.3",
        "nyholm/psr7": "^1.8",
        "relay/relay": "^3.0",
        "symfony/http-client": "^7.1",
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^3.5",
        "symfony/cache": "^7.1",
        "symfony/event-dispatcher": "^7.1",
        "nikic/fast-route": "^1.3",
        "symfony/console": "^7.1",
        "symfony/var-dumper": "^7.1",
        "symfony/var-exporter": "^7.0",
        "filp/whoops": "^2.15",
        "nesbot/carbon": "^3.0",
        "symfony/mailer": "^7.1",
        "symfony/mime": "^7.2",
        "predis/predis": "^2.2",
        "symfony/messenger": "^7.1",
        "cboden/ratchet": "^0.4",
        "doctrine/dbal": "^4.0",
        "doctrine/migrations": "^3.7",
        "symfony/validator": "^7.1",
        "symfony/config": "^7.1",
        "symfony/dotenv": "^7.1",
        "league/flysystem": "^3.28",
        "symfony/password-hasher": "^7.1",
        "symfony/security-csrf": "^7.1",
        "symfony/security-core": "^7.1",
        "symfony/clock": "^7.1",
        "symfony/filesystem": "^7.1",
        "symfony/translation": "^7.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.11",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-doctrine": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "LPwork\\": "src/",
            "App\\": "app/",
            "Config\\": "config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LPwork\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
