# Application identity, environment and base address.

# APP_NAME: application identifier used in logs/metadata; any string.
APP_NAME=LPwork
# APP_VERSION: version label shown in diagnostics; free-form string.
APP_VERSION=0.0.1
# APP_ENV: environment mode (dev/prod/test) controlling debug/safety switches.
APP_ENV=dev
# APP_TIMEZONE: default timezone (PSR-20/Carbon), e.g. UTC or Europe/Warsaw; aliases allowed; empty falls back to UTC.
APP_TIMEZONE=UTC
# APP_LOCALE: default locale/language, e.g. en or pl.
APP_LOCALE=en
# APP_FALLBACK_LOCALE: fallback locale when translation not found.
APP_FALLBACK_LOCALE=en
# APP_SCHEME: base protocol for URL generation (http or https).
APP_SCHEME=http
# APP_URL: full base URL with host and port, e.g. http://localhost:8080.
APP_URL=http://localhost:8080

# PHP runtime settings affecting logging and limits.

# PHP_ERROR_LOG: absolute path to PHP error log file; empty uses php.ini defaults.
PHP_ERROR_LOG=
# PHP_ERROR_REPORTING: error reporting level, e.g. E_ALL or E_ALL & ~E_NOTICE.
PHP_ERROR_REPORTING=E_ALL
# PHP_MEMORY_LIMIT: memory limit, e.g. 256M; -1 disables the limit.
PHP_MEMORY_LIMIT=-1
# PHP_MAX_EXECUTION_TIME: execution time limit in seconds; 0 disables the limit.
PHP_MAX_EXECUTION_TIME=0

# Default database connection for Doctrine DBAL.

# DB_DRIVER: DBAL driver name, e.g. pdo_sqlite, pdo_mysql, pdo_pgsql.
DB_DRIVER=pdo_sqlite
# DB_HOST: database host for network drivers (ignored for SQLite).
DB_HOST=127.0.0.1
# DB_PORT: database port for network drivers (ignored for SQLite).
DB_PORT=3306
# DB_DATABASE: database/schema name for network drivers.
DB_DATABASE=
# DB_USERNAME: database username for network drivers.
DB_USERNAME=
# DB_PASSWORD: database password for network drivers.
DB_PASSWORD=
# DB_PATH: SQLite database path or DSN/host for other drivers.
DB_PATH=storage/db.sqlite3
# DB_CHARSET: character set for supported drivers, e.g. utf8mb4.
DB_CHARSET=utf8mb4

# Filesystem storage settings for the local disk.

# FILESYSTEM_LOCAL_ROOT: root directory for the local filesystem driver.
FILESYSTEM_LOCAL_ROOT=storage

# Redis default connection used by Redis-backed components.

# REDIS_SCHEME: connection scheme, typically tcp or unix.
REDIS_SCHEME=tcp
# REDIS_HOST: Redis host or socket path (when using unix scheme).
REDIS_HOST=127.0.0.1
# REDIS_PORT: Redis port (ignored for unix sockets).
REDIS_PORT=6379
# REDIS_DB: logical Redis database index (0-15 by default).
REDIS_DB=0
# REDIS_USERNAME: Redis username for ACL-enabled servers; empty if unused.
REDIS_USERNAME=
# REDIS_PASSWORD: Redis password/token; empty if no auth.
REDIS_PASSWORD=
# REDIS_PREFIX: key prefix applied by Predis; can stay empty.
REDIS_PREFIX=

# Session configuration: driver selection, cookie policy and storage backends.

# SESSION_DRIVER: active session driver (php, redis, database, filesystem).
SESSION_DRIVER=php
# SESSION_LIFETIME: session lifetime in seconds (cookie Max-Age and storage TTL).
SESSION_LIFETIME=7200
# SESSION_COOKIE_NAME: cookie name carrying the session ID.
SESSION_COOKIE_NAME=LPWORKSESSID
# SESSION_COOKIE_PATH: cookie path scope, typically /.
SESSION_COOKIE_PATH=/
# SESSION_COOKIE_DOMAIN: cookie domain scope; empty for current host.
SESSION_COOKIE_DOMAIN=
# SESSION_COOKIE_SECURE: set secure flag; always forced for HTTPS requests.
SESSION_COOKIE_SECURE=false
# SESSION_COOKIE_HTTP_ONLY: set HttpOnly flag to hide cookie from JS.
SESSION_COOKIE_HTTP_ONLY=true
# SESSION_COOKIE_SAMESITE: SameSite policy (lax/strict/none).
SESSION_COOKIE_SAMESITE=lax
# SESSION_PHP_NAME: native PHP session name when using php driver.
SESSION_PHP_NAME=LPWORKSESSID
# SESSION_REDIS_CONNECTION: Redis connection name for redis session driver.
SESSION_REDIS_CONNECTION=default
# SESSION_REDIS_PREFIX: key prefix for Redis session keys.
SESSION_REDIS_PREFIX=session:
# SESSION_DB_CONNECTION: database connection name for DB session driver (must be default).
SESSION_DB_CONNECTION=default
# SESSION_DB_TABLE: table name storing sessions for DB driver.
SESSION_DB_TABLE=sessions
# SESSION_FILESYSTEM_DISK: filesystem disk name for filesystem session driver.
SESSION_FILESYSTEM_DISK=local
# SESSION_FILESYSTEM_PATH: directory on the disk for session files.
SESSION_FILESYSTEM_PATH=sessions

# Error log configuration: driver selection and storage options.

# ERROR_LOG_DRIVER: error log backend (file, redis, database).
ERROR_LOG_DRIVER=file
# ERROR_LOG_LEVEL: severity filter (reserved, all treated as error for now).
ERROR_LOG_LEVEL=error
# ERROR_LOG_FILE_MODE: file rotation mode (single, daily, monthly).
ERROR_LOG_FILE_MODE=daily
# ERROR_LOG_FILE_DIRECTORY: directory for error log files.
ERROR_LOG_FILE_DIRECTORY=errors
# ERROR_LOG_DB_CONNECTION: database connection name for error log (default recommended).
ERROR_LOG_DB_CONNECTION=default
# ERROR_LOG_DB_TABLE: table name for error log entries.
ERROR_LOG_DB_TABLE=errors
# ERROR_LOG_REDIS_CONNECTION: Redis connection name for error log entries.
ERROR_LOG_REDIS_CONNECTION=default
# ERROR_LOG_REDIS_PREFIX: Redis key prefix/list name for error log entries.
ERROR_LOG_REDIS_PREFIX=errors:
# ERROR_LOG_REDIS_TTL: TTL in seconds for Redis-stored entries (0 = no expiry).
ERROR_LOG_REDIS_TTL=0
# ERROR_LOG_REDIS_MAX_ENTRIES: max list length (trim) for Redis entries (0 = no limit).
ERROR_LOG_REDIS_MAX_ENTRIES=1000
# ERROR_LOG_RESPONSE_EXPOSE_HEADER: whether to add error ID header to responses.
ERROR_LOG_RESPONSE_EXPOSE_HEADER=true
# ERROR_LOG_RESPONSE_HEADER_NAME: header name carrying the error ID.
ERROR_LOG_RESPONSE_HEADER_NAME=X-Error-Id
# ERROR_LOG_RESPONSE_EXPOSE_API_PAYLOAD: whether to include error_id in API JSON payload.
ERROR_LOG_RESPONSE_EXPOSE_API_PAYLOAD=true

# Logging configuration (PSR-3 via Monolog).

# LOG_DEFAULT_CHANNEL: name of the default logging channel (stderr or file).
LOG_DEFAULT_CHANNEL=stderr
# LOG_STDERR_LEVEL: minimum log level for stderr channel (debug/info/notice/warning/error/critical/alert/emergency).
LOG_STDERR_LEVEL=debug
# LOG_FILE_PATH: file path for the single-file logging channel.
LOG_FILE_PATH=storage/logs/app.log
# LOG_FILE_LEVEL: minimum log level for the file channel.
LOG_FILE_LEVEL=info
# LOG_REDIS_CONNECTION: Redis connection name for Redis logging channel.
LOG_REDIS_CONNECTION=default
# LOG_REDIS_KEY: Redis key/list name for log entries.
LOG_REDIS_KEY=logs
# LOG_REDIS_LEVEL: minimum log level for Redis channel.
LOG_REDIS_LEVEL=warning
# LOG_DB_CONNECTION: database connection name for DB logging channel.
LOG_DB_CONNECTION=default
# LOG_DB_TABLE: table name for DB logging channel (must exist).
LOG_DB_TABLE=logs
# LOG_DB_LEVEL: minimum log level for DB channel.
LOG_DB_LEVEL=error

# Cache configuration (PSR-6/16) including routing/config/translation caches.

# CACHE_DEFAULT_POOL: default cache pool name (array/filesystem/redis/pdo or custom).
CACHE_DEFAULT_POOL=array
# CACHE_ARRAY_NAMESPACE: namespace prefix for array cache keys.
CACHE_ARRAY_NAMESPACE=
# CACHE_ARRAY_TTL: default TTL in seconds for array cache (0 or empty means no default expiry).
CACHE_ARRAY_TTL=0
# CACHE_FS_NAMESPACE: namespace prefix for filesystem cache keys.
CACHE_FS_NAMESPACE=
# CACHE_FS_TTL: default TTL in seconds for filesystem cache (0 or empty means no default expiry).
CACHE_FS_TTL=0
# CACHE_FS_PATH: directory path for filesystem cache.
CACHE_FS_PATH=storage/cache
# CACHE_REDIS_CONNECTION: Redis connection name for redis cache pool.
CACHE_REDIS_CONNECTION=default
# CACHE_REDIS_NAMESPACE: namespace prefix for redis cache keys.
CACHE_REDIS_NAMESPACE=
# CACHE_REDIS_TTL: default TTL in seconds for redis cache (0 or empty means no default expiry).
CACHE_REDIS_TTL=0
# CACHE_PDO_CONNECTION: database connection name for PDO cache pool.
CACHE_PDO_CONNECTION=default
# CACHE_PDO_NAMESPACE: namespace prefix for PDO cache keys.
CACHE_PDO_NAMESPACE=
# CACHE_PDO_TTL: default TTL in seconds for PDO cache (0 or empty means no default expiry).
CACHE_PDO_TTL=0
# CACHE_PDO_TABLE: table name for PDO cache items (will be created if possible).
CACHE_PDO_TABLE=cache_items
# ROUTE_CACHE_ENABLED: enable FastRoute dispatcher cache (true/false).
ROUTE_CACHE_ENABLED=false
# ROUTE_CACHE_POOL: cache pool name for route cache.
ROUTE_CACHE_POOL=filesystem
# ROUTE_CACHE_KEY: cache key for stored dispatcher data.
ROUTE_CACHE_KEY=routes:dispatcher
# CONFIG_CACHE_ENABLED: enable configuration cache (true/false).
CONFIG_CACHE_ENABLED=false
# CONFIG_CACHE_POOL: cache pool name for configuration cache.
CONFIG_CACHE_POOL=filesystem
# CONFIG_CACHE_KEY: cache key for configuration repository payload.
CONFIG_CACHE_KEY=config:repository
# TRANSLATION_CACHE_ENABLED: enable translation catalogue cache (true/false).
TRANSLATION_CACHE_ENABLED=true
# TRANSLATION_CACHE_POOL: cache pool name for translation catalogues.
TRANSLATION_CACHE_POOL=filesystem
# TRANSLATION_CACHE_PREFIX: key prefix for translation cache entries.
TRANSLATION_CACHE_PREFIX=translations:

# HTTP client defaults (PSR-18 via Symfony HttpClient).

# HTTP_CLIENT_BASE_URI: base URI for outgoing requests; empty to disable.
HTTP_CLIENT_BASE_URI=
# HTTP_CLIENT_TIMEOUT: request timeout in seconds.
HTTP_CLIENT_TIMEOUT=30
# HTTP_CLIENT_MAX_REDIRECTS: max redirects to follow.
HTTP_CLIENT_MAX_REDIRECTS=10
# HTTP_CLIENT_VERIFY: enable TLS peer/host verification (true/false).
HTTP_CLIENT_VERIFY=true

# HTTP body parsing and CORS settings.

# HTTP_BODY_PARSING_ENABLED: enable body parsing middleware (true/false).
HTTP_BODY_PARSING_ENABLED=true
# HTTP_BODY_MAX_SIZE: max accepted body size in bytes (0 = unlimited).
HTTP_BODY_MAX_SIZE=1048576
# HTTP_BODY_REJECT_INVALID_JSON: return 400 on invalid JSON (true/false).
HTTP_BODY_REJECT_INVALID_JSON=true
# HTTP_BODY_JSON_MAX_DEPTH: max depth for JSON decode.
HTTP_BODY_JSON_MAX_DEPTH=512
# HTTP_BODY_JSON_ASSOC: decode JSON objects as arrays (true/false).
HTTP_BODY_JSON_ASSOC=false
# HTTP_CORS_ENABLED: enable CORS middleware (true/false).
HTTP_CORS_ENABLED=false
# HTTP_CORS_ALLOW_ORIGIN: allowed origins (comma-separated, "*" allowed).
HTTP_CORS_ALLOW_ORIGIN=*
# HTTP_CORS_ALLOW_METHODS: allowed methods (comma-separated).
HTTP_CORS_ALLOW_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS
# HTTP_CORS_ALLOW_HEADERS: allowed request headers (comma-separated).
HTTP_CORS_ALLOW_HEADERS=Content-Type,Authorization
# HTTP_CORS_EXPOSE_HEADERS: response headers exposed to the client (comma-separated, can be empty).
HTTP_CORS_EXPOSE_HEADERS=
# HTTP_CORS_ALLOW_CREDENTIALS: allow credentials (true/false).
HTTP_CORS_ALLOW_CREDENTIALS=false
# HTTP_CORS_MAX_AGE: preflight cache max age in seconds.
HTTP_CORS_MAX_AGE=600

# Mail transports (Symfony Mailer).

# MAIL_DEFAULT_CONNECTION: default mail connection name.
MAIL_DEFAULT_CONNECTION=smtp
# MAIL_DSN: DSN string for the default mailer (e.g. smtp://user:pass@host:587?encryption=tls).
MAIL_DSN=smtp://localhost
# MAIL_TIMEOUT: mailer timeout in seconds (0 or empty = default).
MAIL_TIMEOUT=0

# Security (CSRF + headers).

# SECURITY_CSRF_ENABLED: enable CSRF verification (true/false).
SECURITY_CSRF_ENABLED=true
# SECURITY_CSRF_TOKEN_ID: token ID/namespace.
SECURITY_CSRF_TOKEN_ID=default
# SECURITY_CSRF_HEADER: header name carrying CSRF token.
SECURITY_CSRF_HEADER=X-CSRF-Token
# SECURITY_CSRF_PARAMETER: body/query parameter carrying CSRF token.
SECURITY_CSRF_PARAMETER=_csrf
# SECURITY_CSRF_METHODS: methods requiring CSRF (comma-separated).
SECURITY_CSRF_METHODS=POST,PUT,PATCH,DELETE
# SECURITY_CSRF_EXCLUDE_PATHS: paths excluded from CSRF (comma-separated prefixes).
SECURITY_CSRF_EXCLUDE_PATHS=
# SECURITY_HEADERS_ENABLED: add security headers (true/false).
SECURITY_HEADERS_ENABLED=true
# SECURITY_FRAME_OPTIONS: X-Frame-Options value (SAMEORIGIN or DENY).
SECURITY_FRAME_OPTIONS=SAMEORIGIN
# SECURITY_REFERRER_POLICY: Referrer-Policy header value.
SECURITY_REFERRER_POLICY=no-referrer
# SECURITY_PERMISSIONS_POLICY: Permissions-Policy value (empty to skip).
SECURITY_PERMISSIONS_POLICY=
# SECURITY_CONTENT_TYPE_OPTIONS: add X-Content-Type-Options: nosniff (true/false).
SECURITY_CONTENT_TYPE_OPTIONS=true

# Queue configuration: default queue and sample driver settings.

# QUEUE_DEFAULT: name of the default queue used by enqueue APIs.
QUEUE_DEFAULT=default
# QUEUE_DEFAULT_DRIVER: driver for the default queue (redis, database, filesystem).
QUEUE_DEFAULT_DRIVER=redis
# QUEUE_DEFAULT_SERIALIZER: serializer id for jobs (json by default).
QUEUE_DEFAULT_SERIALIZER=json
# QUEUE_DEFAULT_CONNECTION: connection name for the default queue (redis/db/filesystem).
QUEUE_DEFAULT_CONNECTION=default
# QUEUE_DEFAULT_REDIS_MODE: redis mode (list or stream).
QUEUE_DEFAULT_REDIS_MODE=list
# QUEUE_DEFAULT_REDIS_KEY: redis key for the queue.
QUEUE_DEFAULT_REDIS_KEY=queues:default
# QUEUE_DEFAULT_REDIS_GROUP: consumer group name for stream mode.
QUEUE_DEFAULT_REDIS_GROUP=lpwork
# QUEUE_DEFAULT_REDIS_CONSUMER: consumer name for stream mode.
QUEUE_DEFAULT_REDIS_CONSUMER=worker
# QUEUE_DEFAULT_REDIS_BLOCK: block timeout in seconds for stream reads.
QUEUE_DEFAULT_REDIS_BLOCK=5
# QUEUE_DEFAULT_DB_TABLE: table storing jobs for database driver.
QUEUE_DEFAULT_DB_TABLE=queue_jobs
# QUEUE_DEFAULT_FS_PATH: filesystem path for filesystem queue jobs.
QUEUE_DEFAULT_FS_PATH=storage/queue
# QUEUE_RETRY_ENABLED: enable retry middleware for queue jobs (true/false).
QUEUE_RETRY_ENABLED=true
# QUEUE_RETRY_MAX_RETRIES: maximum number of retries before giving up.
QUEUE_RETRY_MAX_RETRIES=3
# QUEUE_RETRY_DELAY_MS: initial retry delay in milliseconds.
QUEUE_RETRY_DELAY_MS=1000
# QUEUE_RETRY_MULTIPLIER: backoff multiplier applied to delay (e.g. 2 doubles).
QUEUE_RETRY_MULTIPLIER=2.0
# QUEUE_RETRY_MAX_DELAY_MS: maximum retry delay in milliseconds (0 = no cap).
QUEUE_RETRY_MAX_DELAY_MS=0

# WebSocket servers (Ratchet).

# WEBSOCKET_DEFAULT: default server name.
WEBSOCKET_DEFAULT=default
# WEBSOCKET_DEFAULT_ENABLED: enable default server (true/false).
WEBSOCKET_DEFAULT_ENABLED=true
# WEBSOCKET_DEFAULT_HOST: bind host for default server.
WEBSOCKET_DEFAULT_HOST=0.0.0.0
# WEBSOCKET_DEFAULT_PORT: bind port for default server.
WEBSOCKET_DEFAULT_PORT=8081
# WEBSOCKET_DEFAULT_ORIGINS: allowed origins (comma-separated, "*" allows all).
WEBSOCKET_DEFAULT_ORIGINS=*
